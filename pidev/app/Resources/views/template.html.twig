<script>
    function getOccu(array,id) {
        var nbOcc = 0;
        for (var i = 0; i < array.length; i++) {
            if (array[i] == id) {
                nbOcc++;
            }
        }
        return nbOcc
    }

    function updatepanier (){
        $("#cartBody").html("")
        $.ajax({url: "http://localhost/pidev/web/app_dev.php/boutique/getSession", success: function(result){
            var array=result.data
            var notDupli= []
            $.each(array, function(i, el){
                if($.inArray(el, notDupli) === -1) {notDupli.push(el);}
            });

            $.each(notDupli,function (i,el) {

                $.ajax({async:true,url: "http://localhost/pidev/web/app_dev.php/boutique/getProduct/"+el, success: function(p){

                   $("#cartBody").append(

                           '<div class="media">'+
                           '<div class="media-left">'+
                           '<img alt="" src="http://localhost/pidev/web/'+
                           p.image+
                           '" width="80%" height="80%"></div>'+
                           '<div class="media-body">'+
                           '<h4>'+p.nom+'</h4>'+
                           '<div class="price"> '+
                           '<span class="color-1">'+p.prix+'</span>'+
                           '</div> '+
                           '<span class="color-2">Qnt: '+getOccu(array,p.id)+'</span>'+
                           '</div>'+
                           '</div>'+

                           '<span class="cancel"><i class="fa fa-close" aria-hidden="true"></i></span>'+
                           '</li>'

                    );


                }
                })
            });


            $("#countCart").html('<i  class="fa fa-shopping-basket " aria-hidden="true"></i> '+result.data.length+' produits dans votre panier')

            $("#nbr").html('<span class="badge bg-color-1" >'+result.data.length+'</span>')




        }});

    }

    updatepanier()


</script>
